                "produces": [
                    "application/json"
                ],
                "summary": "add a new problem",
                "parameters": [
                    {
                        "type": "string",
                        "description": "problem title",
                        "name": "title",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "integer",
                        "description": "problem memory limitation",
                        "name": "memo_limit",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "integer",
                        "description": "problem time limitation",
                        "name": "time_limit",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "problem text",
                        "name": "text",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {}
            }
        },
        "/problem/delete": {
            "post": {
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "delete a problem",
